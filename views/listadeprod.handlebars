    {{#each datos}}
    <div class="tarjetas">
        <img src="/imagenes/parte-arriba/{{this.foto}}" id="img-prods" >
        <p>{{this.nombre}}</p>
        <a href="/detalleproducto?id={{this.id}}">Comprar: ${{this.precioC}}</button>
        <button id="alquila">Alquilar: ${{this.precioA}}</button>
    </div>
    {{/each}}
